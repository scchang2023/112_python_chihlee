    "import math\n",
    "n = float(input('請輸入斜邊：'))\n",
    "m = float(input('請輸入對邊：'))\n",
    "radion = math.asin(m / n)\n",
    "degree = round(math.degrees(radion))\n",
    "# 字串插補，f-format\n",
    "print(f'直角三角形的角度為：{degree}度')"
   ]
